<%=link_to "Month Calendar", month_path, type: "button", class: 'btn btn-primary'%>
<%= week_calendar(events: @tasks) do |date, tasks| %>
  <%= date %>

  <% tasks.each do |tasks| %>
  <%if tasks.user_id == current_user.id%>
    <% if tasks.completed %>
      <div style="text-decoration: line-through">
        <%= tasks.title %>
      </div>
    <%else%>
      <div>
        <%= tasks.title %>
      </div>
    <%end%>
  <% end %>
  <%end%>
<% end %>