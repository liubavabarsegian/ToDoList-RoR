<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">
  <% @users.each do |user| %>
    <div id="<%= dom_id user %>" class = "user">
      <p>
        <strong>Nick:</strong>
        <%= user.nick %>
      </p>
      <p>
      <%= link_to "Show this user", user, class: 'link'%>
    </p>
    </div>
  <% end %>
</div>
